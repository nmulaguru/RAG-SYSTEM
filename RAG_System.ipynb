        "        for page in doc:\n",
        "            full_text.append(page.get_text())\n",
        "    text = \"\\n\".join(full_text)\n",
        "\n",
        "    with open(output_txt, \"w\", encoding=\"utf-8\") as f:\n",
        "        f.write(text)\n",
        "\n",
        "    return text"
