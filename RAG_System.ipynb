        "def is_safe_query(query):\n",
        "    banned_keywords = ['kill', 'hack', 'violence', 'sex', 'bomb', 'politics', 'suicide']\n",
        "    return not any(word in query.lower() for word in banned_keywords)\n",
        "\n",
        "def is_indian_food_query(query):\n",
        "    keywords = ['recipe', 'dish', 'cook', 'ingredients', 'spice', 'masala', 'dal', 'biryani', 'chutney', 'curry']\n",
        "    return any(word in query.lower() for word in keywords)\n",
        "\n",
        "def validate_query(query):\n",
        "    if not is_safe_query(query):\n",
        "        return False, \"Sorry, your query isn't allowed.\"\n",
        "    if not is_indian_food_query(query):\n",
        "        return False, \"I can only help with Indian cooking questions.\"\n",
        "    return True, \"\""
