      "source": [
        "def augmented_query(user_query, embed_model = \"text-embedding-3-small\", k=10):\n",
        "  # NNG: Get the top_k contexts and the query, given the input query.\n",
        "  contexts, query = get_context(user_query, embed_model=embed_model, k=k)\n",
        "  # NNG: Return contexts and the query seperated by dashes.\n",
        "  return \"\\n\\n---------\\n\\n\".join(contexts) + \"\\n\\n -------- \\n\\n\" + query"
